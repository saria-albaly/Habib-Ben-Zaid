            SELECT * FROM document_sections 
		        LEFT JOIN document_section_versions
                on document_section_id = document_sections.id
                WHERE document_section_versions.id = (
                	SELECT MAX(id) FROM document_section_versions WHERE document_section_id = document_sections.id GROUP BY document_section_id
                )



